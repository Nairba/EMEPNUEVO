@model IEnumerable<EMEP.Models.Paciente>

@{
    ViewBag.Title = "Indexp";
    Layout = "~/Views/Shared/_LayoutPa.cshtml";
}

<h2>Indexp</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.s_Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contrasenna)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sexo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo_Usuario.descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cedula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.p_Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.s_Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contrasenna)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sexo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_Usuario.descripcion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.correo }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.correo })
        </td>
    </tr>
}

</table>
