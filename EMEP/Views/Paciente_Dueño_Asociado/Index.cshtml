@model IEnumerable<EMEP.Models.Paciente_Dueño_Asociado>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPa.cshtml";
}

<h2>Lista de Pacientes Asociados</h2>

<p>
    @Html.ActionLink("Nuevo Paciente Asociado", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_PACIENTE_DUE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID_PACIENTE_ASO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_PACIENTE_DUE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID_PACIENTE_ASO)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id }) |
            @Html.ActionLink("Remover", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
